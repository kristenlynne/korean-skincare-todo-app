                <a href="/logout" class="logout">Logout</a>
               </div>
               
        </header>
        <!-- End of header -->

    <section id="morning-routine" class="routine">
        <section class="routine-selector-container">
            <h2 class="morning-selected"><a href="/todos/morningTodos"><i class="fa-solid fa-sun"></i></a></h2>
            <h2 class="night-selected"><a href="/todos/nightTodos"><i class="fa-solid fa-moon"></i></a></h2>
        </section>
    
        <h1 class="todoRoutine">✨ Welcome <%= user.firstName %>, here's your evening skincare routine.</h1>
        
        <section class="checklist-container">
            <ul>
                <% todos.forEach( el => { %>
                    <% if (el.isNight) { %>
                        <li class='todoItem' data-id='<%=el._id%>'>
                            <span class='<%= el.completed === true ? 'completed' : 'not'%>'><%= el.todo %></span>
                            <i class="fa-solid <%= el.completed === true ? 'fa-circle-check' : 'fa-circle' %>"></i>
                            <i class="fa-solid fa-trash-can del"></i>
                        </li>
                    <% } %>
                <% }) %>    
            </ul>
            <form action="/todos/createNightTodo" method='POST'>
                <input class="todoInput" type="text" placeholder="Enter Todo Item" name='todoItem' autocomplete="off">
                <button type="submit"><i class="fa-solid fa-plus"></i></button>
            </form>    
        </section>
        <section class="steps-left">
              
            <% if (nightLeft === 0) { %>
                <h2>You completed your evening routine, see you tomorrow night!</h2>
            <% } else if (nightLeft === 1) { %>
                <h2>You have <%= nightLeft %> step left to do.</h2>
            <% } else { %>
                <h2>You have <%= nightLeft %> steps left to do.</h2> 
            <% } %>           

            <span class="reset">
                <button type="">
                <a action="/todos/resetTodo">Reset</a>
            </button>
            </span>

        </section>

    </section>

    <footer class="morningFooter"> <i class="fa-regular fa-copyright"></i>
        <span>Group project 100Devs 2022</span>
    </footer>

  </main>
      
    <script src="/js/main.js"></script>
